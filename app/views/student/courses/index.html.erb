<div class="d-flex">
  <h1>Courses</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Notes</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.title %></td>
        <td><%= course.description %></td>
        <td><%= course.notes %></td>
        <td><%= course.start_time.strftime("%I:%M %p") %></td>
        <td><%= course.end_time.strftime("%I:%M %p") %></td>
        <td><%= link_to 'Show', [:student, course] %></td>
        <td><%= link_to "Apply",[:student, course] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
